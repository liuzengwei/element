{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    ["module-resolver", {
      "root": ["@liuzengwei/element-ui"],
      "alias": {
        "element-ui/src": "@liuzengwei/element-ui/lib",
        "@liuzengwei/element-ui/src": "@liuzengwei/element-ui/lib"
      }
    }]
  ],
  "env": {
    "utils": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": "commonjs",
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
          }
        ],
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["@liuzengwei/element-ui"],
          "alias": {
            "element-ui/src": "@liuzengwei/element-ui/lib"
          }
        }]
      ]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  }
}
